{
  "manifest_version": 3,
  "name": "Comic Sans Everywhere",
  "version": "1.0",
  "description": "Turns all webpage text to Comic Sans.",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}